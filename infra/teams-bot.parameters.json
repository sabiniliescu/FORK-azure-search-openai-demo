{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "botName": {
      "value": "${AZURE_TEAMS_BOT_NAME}"
    },
    "appServicePlanName": {
      "value": "${AZURE_TEAMS_BOT_PLAN_NAME}"
    },
    "webAppName": {
      "value": "${AZURE_TEAMS_BOT_WEBAPP_NAME}"
    },
    "microsoftAppId": {
      "value": "${MICROSOFT_APP_ID}"
    },
    "microsoftAppPassword": {
      "value": "${MICROSOFT_APP_PASSWORD}"
    },
    "backendUrl": {
      "value": "${BACKEND_URL}"
    },
    "appServicePlanSku": {
      "value": "${AZURE_TEAMS_BOT_SKU=B1}"
    }
  }
}
